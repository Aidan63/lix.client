{
    "version": "0.1.0",
    "command": "haxe",
    "args": ["lix.client.hxml", "--next", "-cmd", "node bin/lix.js download https://lib.haxe.org/p/tink_state/0.3.0/download/ as tink_state#0.4.2"],
    "problemMatcher": {
        "owner": "haxe",
        "pattern": {
            "regexp": "^(.+):(\\d+): (?:lines \\d+-(\\d+)|character(?:s (\\d+)-| )(\\d+)) : (?:(Warning) : )?(.*)$",
            "file": 1,
            "line": 2,
            "endLine": 3,
            "column": 4,
            "endColumn": 5,
            "severity": 6,
            "message": 7
        }
    }
}
