{
    "version": "0.1.0",
    "command": "haxe",
    "args": ["lix.client.hxml", "--next", "-cmd", "node bin/lix.js download https://github.com/haxetink/tink_chunk/archive/69a8247941c0135f1b7b2a4f5ce9518932b0d274.tar.gz into tink_chunk/0.1.0/github/69a8247941c0135f1b7b2a4f5ce9518932b0d274"],
    "problemMatcher": {
        "owner": "haxe",
        "pattern": {
            "regexp": "^(.+):(\\d+): (?:lines \\d+-(\\d+)|character(?:s (\\d+)-| )(\\d+)) : (?:(Warning) : )?(.*)$",
            "file": 1,
            "line": 2,
            "endLine": 3,
            "column": 4,
            "endColumn": 5,
            "severity": 6,
            "message": 7
        }
    }
}
